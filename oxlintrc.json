{
  "$schema": "./node_modules/oxlint/configuration_schema.json",
  "plugins": ["react", "typescript", "nextjs"],
  "categories": {
    "correctness": "warn",
    "suspicious": "warn"
  },
  "rules": {
    "no-unused-vars": "off",
    "typescript/no-unused-vars": "off",
    "react/react-in-jsx-scope": "off"
  }
}
